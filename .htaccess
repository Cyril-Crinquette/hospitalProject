# links follow
Options +FollowSymlinks 
# rewrite Apache activate
RewriteEngine On


# /accueil -> /controllers/page-controller-home.php
 RewriteRule ^accueil$ /controllers/page-controller-home.php [L]

# /inscription -> /controllers/page-controller-add-patient.php
 RewriteRule ^inscription$ /controllers/page-controller-add-patient.php [L]

 # /liste-de-patients -> /controllers/page-controller-patients-list.php
 RewriteRule ^liste-de-patients$ /controllers/page-controller-patients-list.php [L]

  # /erreur -> /controllers/page-controller-error.php
 RewriteRule ^erreur$ /controllers/page-controller-error.php [L]

  # /profil-patient -> /controllers/page-controller-patient-profile.php
 RewriteRule ^profil-patient$ /controllers/page-controller-patient-profile.php [L]

 # /modification -> /controllers/page-controller-modify.php
 RewriteRule ^modification$ /controllers/page-controller-modify.php [L]





